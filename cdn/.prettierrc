{
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 120,
  "arrowParens": "always",
  "endOfLine": "lf",
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "overrides": [
    {
      "files": ["*.json", "*.yaml", "*.yml", ".prettierrc"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
